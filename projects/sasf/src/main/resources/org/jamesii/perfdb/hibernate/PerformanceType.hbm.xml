<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.jamesii.perfdb.hibernate">
  <class name="PerformanceType" table="performance_types">
    <id name="ID">
      <generator class="native" />
    </id>
    <property name="name" length="64" />
    <property name="description" />
    <property name="performanceMeasurerFactory" column="performance_measurer" />
  </class>
</hibernate-mapping>