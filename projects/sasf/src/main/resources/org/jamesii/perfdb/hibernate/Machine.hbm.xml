<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.jamesii.perfdb.hibernate">
  <class name="Machine" table="machines">
    <id name="ID">
      <generator class="native" />
    </id>
    <property name="name" length="128" />
    <property name="description" />
    <property name="macAddress" column="mac_address" />
    <property name="javaSciMark" column="java_scimark" />
  </class>
</hibernate-mapping>