<?xml version="1.0" encoding="UTF-8" ?> 
<plugintype xmlns="http://www.jamesii.org/plugintype" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.jamesii.org/plugintype http://www.jamesii.org/plugintype/plugintype.xsd">
  <id name="random generator plugins" version="1.0" icon="random.png"/> 
  <abstractfactory>org.jamesii.core.math.random.generators.plugintype.AbstractRandomGeneratorFactory</abstractfactory> 
  <basefactory>org.jamesii.core.math.random.generators.plugintype.RandomGeneratorFactory</basefactory>
  <parameter name="seed" type="java.lang.Long">
     <description>The seed to be used for the initialization of the random generator. If not given the system's default seed generation mechanism will be used instead.</description>
